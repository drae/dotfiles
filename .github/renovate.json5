{
  extends: [
    'config:recommended',
    ':dependencyDashboard',
    ':disableRateLimiting',
    ':enablePreCommit',
    ':timezone(Europe/London)',
  ],
  gitAuthor: 'drae-bot <107273482+drae-bot[bot]@users.noreply.github.com>',
  dependencyDashboardTitle: 'Renovate Dashboard ðŸ¤–',
  baseBranchPatterns: [
    'main',
  ],
  customManagers: [
    {
      customType: 'regex',
      description: [
        'mise package regex manager',
      ],
      managerFilePatterns: [
        '/home\\/private_dot_config\\/mise\\/config.toml$/',
      ],
      matchStrings: [
        '("\\S+:(\\S+/)?)?(?<depName>\\S+?)"? = \'(?<currentValue>[^\\s]*?)\'[ \\t]*?#renovate:( datasource=(?<datasource>.*?))?( packageName=(?<packageName>.*?))?( versioning=(?<versioning>.*?=?))?( versionTemplate=(?<versionTemplate>.*?=?))?([\\s\\t]+)?\\n',
      ],
      packageNameTemplate: '{{#if packageName}}{{{packageName}}}{{else}}{{/if}}',
      datasourceTemplate: '{{#if datasource}}{{{datasource}}}{{else}}github-releases{{/if}}',
      versioningTemplate: '{{#if versioning}}{{{versioning}}}{{else}}semver{{/if}}',
      extractVersionTemplate: '{{#if versionTemplate}}{{{versionTemplate}}}{{else}}^v?(?<version>.*)${{/if}}',
    },
    {
      customType: 'regex',
      description: [
        'mise binary installation script manager',
      ],
      managerFilePatterns: [
        '/\\.chezmoiscripts/.*\\.sh\\.tmpl$/',
      ],
      matchStrings: [
        '# renovate\\s*\\n(\\s*)curl -Ls https://github\\.com/jdx/mise/releases/download/v(?<currentValue>[^/]+)/mise-v[^-]+-linux-x64 --create-dirs -o \\$HOME/\\.local/bin/mise',
      ],
      datasourceTemplate: 'github-releases',
      depNameTemplate: 'jdx/mise',
      versioningTemplate: 'semver',
      autoReplaceStringTemplate: '# renovate\n  curl -Ls https://github.com/jdx/mise/releases/download/v{{newValue}}/mise-v{{newValue}}-linux-x64 --create-dirs -o $HOME/.local/bin/mise',
    },
  ],
}
